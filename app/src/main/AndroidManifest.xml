<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wangenyong.dsfarm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".module.base.DSFarmApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".module.main.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module.customview.CustomViewActivity"
            android:parentActivityName=".module.main.MainActivity" />
        <activity
            android:name=".module.webview.WebViewActivity"
            android:parentActivityName=".module.main.MainActivity"/>
    </application>

</manifest>